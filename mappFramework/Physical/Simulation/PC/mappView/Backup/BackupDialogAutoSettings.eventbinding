<?xml version="1.0" encoding="utf-8"?>
<EventBindingSet id="BackupDialogAutoSettings" xmlns="http://www.br-automation.com/iat2014/eventbinding/v2" xmlns:types="http://www.br-automation.com/iat2015/widgetTypes/v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Bindings>
    <EventBinding id="BackupDialogAutoSettings.BtnBackupDialogSave.Click">
      <Source xsi:type="widgets.brease.Button.Event" contentRefId="BackupDialogAutoSettings" widgetRefId="BtnBackupDialogSave" event="Click" />
      <EventHandler>
        <Sequence>
          <Step order="1">
            <Action>
              <Target xsi:type="opcUa.NodeAction" refId="::BackupMgr:HmiBackup.Commands.SaveConfig">
                <Method xsi:type="opcUa.NodeAction.SetValueBool" value="true" />
              </Target>
            </Action>
          </Step>
          <Step order="2">
            <Action>
              <Target xsi:type="clientSystem.Action">
                <Method xsi:type="clientSystem.Action.CloseDialog" dialogId="BackupAutoSettings" />
              </Target>
            </Action>
          </Step>
        </Sequence>
      </EventHandler>
    </EventBinding>
    <EventBinding id="BackupDialogAutoSettings.BtnBackupDialogCancel.Click">
      <Source xsi:type="widgets.brease.Button.Event" contentRefId="BackupDialogAutoSettings" widgetRefId="BtnBackupDialogCancel" event="Click" />
      <EventHandler>
        <Action>
          <Target xsi:type="clientSystem.Action">
            <Method xsi:type="clientSystem.Action.CloseDialog" dialogId="BackupAutoSettings" />
          </Target>
        </Action>
      </EventHandler>
    </EventBinding>
  </Bindings>
</EventBindingSet>
